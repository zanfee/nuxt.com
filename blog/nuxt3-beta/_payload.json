hildren":875},{"href":873,"rel":874},"https://nuxt.com",[847],[876],{"type":840,"value":873},{"type":840,"value":878},".",{"type":835,"tag":880,"props":881,"children":883},"h2",{"id":882},"a-new-foundation",[884],{"type":840,"value":885},"A new foundation",{"type":835,"tag":836,"props":887,"children":888},{},[889,891,898,900,906,908,915],{"type":840,"value":890},"On top of supporting ",{"type":835,"tag":843,"props":892,"children":895},{"href":893,"rel":894},"https://vuejs.org",[847],[896],{"type":840,"value":897},"Vue 3",{"type":840,"value":899}," or ",{"type":835,"tag":843,"props":901,"children":904},{"href":902,"rel":903},"https://vitejs.dev",[847],[905],{"type":840,"value":654},{"type":840,"value":907},", Nuxt 3 contains a new ",{"type":835,"tag":843,"props":909,"children":912},{"href":910,"rel":911},"https://nuxt.com/docs/guide/concepts/server-engine",[847],[913],{"type":840,"value":914},"server engine",{"type":840,"value":916},", unlocking new full-stack capabilities to Nuxt server and beyond. It's the first JavaScript application server that is portable across a variety of modern cloud hosting providers.",{"type":835,"tag":836,"props":918,"children":919},{},[920,922,928],{"type":840,"value":921},"In production, it builds your Vue application and server into one universal ",{"type":835,"tag":923,"props":924,"children":926},"code",{"className":925},[],[927],{"type":840,"value":129},{"type":840,"value":929}," directory. This output is light: minified and without any other Node.js dependencies (except polyfills). You can deploy this output on any system supporting JavaScript, whether Node.js, Serverless, Workers, Edge-side rendering or purely static.",{"type":835,"tag":836,"props":931,"children":932},{},[933,939,941,948],{"type":835,"tag":934,"props":935,"children":936},"strong",{},[937],{"type":840,"value":938},"Bonus:",{"type":840,"value":940}," this server engine can be used on existing Nuxt 2 projects with ",{"type":835,"tag":843,"props":942,"children":945},{"href":943,"rel":944},"https://nuxt.com/docs/getting-started/bridge",[847],[946],{"type":840,"value":947},"Nuxt Bridge",{"type":840,"value":949}," üöÄ",{"type":835,"tag":836,"props":951,"children":952},{},[953,955,961],{"type":840,"value":954},"Head over the ",{"type":835,"tag":843,"props":956,"children":958},{"href":873,"rel":957},[847],[959],{"type":840,"value":960},"Nuxt 3 homepage",{"type":840,"value":962}," to learn more about Nuxt Nitro and Nuxt Bridge.",{"type":835,"tag":880,"props":964,"children":966},{"id":965},"important-notes",[967],{"type":840,"value":968},"Important notes",{"type":835,"tag":836,"props":970,"children":971},{},[972,974,981],{"type":840,"value":973},"Nuxt 3 is currently in beta, so expect things to break (and be fixed quickly). We have ",{"type":835,"tag":843,"props":975,"children":978},{"href":976,"rel":977},"https://github.com/nuxt/nuxt/issues",[847],[979],{"type":840,"value":980},"plenty of work left",{"type":840,"value":982}," but we want to open it publicly to gather feedback and contributions from the community üíö",{"type":835,"tag":836,"props":984,"children":985},{},[986],{"type":835,"tag":934,"props":987,"children":988},{},[989],{"type":840,"value":990},"Do not use it for production until we reach the first release candidate.",{"type":835,"tag":836,"props":992,"children":993},{},[994,996,1003,1005,1012],{"type":840,"value":995},"During the beta, almost every commit will ",{"type":835,"tag":843,"props":997,"children":1000},{"href":998,"rel":999},"https://github.com/nuxt/nuxt/blob/main/.github/workflows/ci.yml#L111-L119",[847],[1001],{"type":840,"value":1002},"trigger a new npm release",{"type":840,"value":1004},"; you may want to look at the ",{"type":835,"tag":843,"props":1006,"children":1009},{"href":1007,"rel":1008},"https://github.com/nuxt/nuxt/pulls?q=is%3Apr+is%3Amerged",[847],[1010],{"type":840,"value":1011},"merged pull requests",{"type":840,"value":1013}," until we begin generating automated changelogs in the documentation.",{"type":835,"tag":836,"props":1015,"children":1016},{},[1017],{"type":840,"value":1018},"We are working every day to improve the documentation, explaining as much as possible all the concepts, features and usage of Nuxt 3.",{"type":835,"tag":836,"props":1020,"children":1021},{},[1022,1024,1031,1033,1040,1041,1048],{"type":840,"value":1023},"Check out the community section of the Nuxt 3 website for ",{"type":835,"tag":843,"props":1025,"children":1028},{"href":1026,"rel":1027},"https://nuxt.com/docs/community/getting-help",[847],[1029],{"type":840,"value":1030},"getting help",{"type":840,"value":1032},", ",{"type":835,"tag":843,"props":1034,"children":1037},{"href":1035,"rel":1036},"https://nuxt.com/docs/community/reporting-bugs",[847],[1038],{"type":840,"value":1039},"reporting bugs",{"type":840,"value":899},{"type":835,"tag":843,"props":1042,"children":1045},{"href":1043,"rel":1044},"https://nuxt.com/docs/community/contribution",[847],[1046],{"type":840,"value":1047},"contributing to the framework",{"type":840,"value":878},{"type":835,"tag":880,"props":1050,"children":1052},{"id":1051},"timeline",[1053],{"type":840,"value":1054},"Timeline",{"type":835,"tag":836,"props":1056,"children":1057},{},[1058],{"type":840,"value":1059},"Here some major milestones we've achieved on the way to Nuxt 3:",{"type":835,"tag":1061,"props":1062,"children":1063},"list",{},[1064],{"type":835,"tag":1065,"props":1066,"children":1067},"ul",{},[1068,1079,1089,1112,1128,1146,1162,1179,1197,1216,1244,1265,1275,1285,1303,1313],{"type":835,"tag":1069,"props":1070,"children":1071},"li",{},[1072,1077],{"type":835,"tag":934,"props":1073,"children":1074},{},[1075],{"type":840,"value":1076},"Jul 2, 2020",{"type":840,"value":1078},": Nuxt 3 first commit with full TypeScript rewrite",{"type":835,"tag":1069,"props":1080,"children":1081},{},[1082,1087],{"type":835,"tag":934,"props":1083,"children":1084},{},[1085],{"type":840,"value":1086},"Aug 7, 2020",{"type":840,"value":1088},": Webpack 5 support"