:811,"category":812,"type":813,"maintainers":814,"compatibility":817,"generatedAt":820,"stats":821,"contributors":829,"readme":843},"cloudcms","CloudCMS Nuxt Module","gitana/cloudcms-nuxt","cloudcms-nuxt","","https://github.com/gitana/cloudcms-nuxt","https://www.cloudcms.com/","CMS","3rd-party",[815],{"name":816,"github":816},"gitana",{"nuxt":818,"requires":819},"^2.0.0",{},"2024-06-13T16:11:39.056Z",{"version":822,"downloads":823,"stars":824,"watchers":824,"forks":825,"defaultBranch":826,"publishedAt":827,"createdAt":828},"1.2.4",17,1,0,"master",1649746123967,1549661640587,[830,834,837,840],{"id":831,"username":832,"contributions":833},5482982,"mibewh",15,{"id":835,"username":805,"contributions":836},436124,2,{"id":838,"username":839,"contributions":824},362824,"uzquiano",{"id":841,"username":842,"contributions":824},8740221,"mteasdale1",{"data":844,"body":847,"toc":1292},{"title":845,"description":846},"Cloud CMS Nuxt Module","This module makes it easy to use Cloud CMS from within your Nuxt JS pages and components.",{"type":848,"children":849},"root",[850,858,863,868,888,901,906,912,922,928,941,950,955,963,968,1020,1026,1071,1206,1225,1233,1238,1265,1278,1286],{"type":851,"tag":852,"props":853,"children":855},"element","h1",{"id":854},"cloud-cms-nuxt-module",[856],{"type":857,"value":845},"text",{"type":851,"tag":859,"props":860,"children":861},"p",{},[862],{"type":857,"value":846},{"type":851,"tag":859,"props":864,"children":865},{},[866],{"type":857,"value":867},"It provides the following:",{"type":851,"tag":869,"props":870,"children":871},"ol",{},[872,878,883],{"type":851,"tag":873,"props":874,"children":875},"li",{},[876],{"type":857,"value":877},"An initialized connect to Cloud CMS for use in query, search and retrieval of content.",{"type":851,"tag":873,"props":879,"children":880},{},[881],{"type":857,"value":882},"Optional support for Preview Mode (providing your editors with instant preview of editorial changes from within Cloud CMS).",{"type":851,"tag":873,"props":884,"children":885},{},[886],{"type":857,"value":887},"Optional support for Page Renditions (allowing your editors to see and preview precisely where a piece of content appears on the site).",{"type":851,"tag":859,"props":889,"children":890},{},[891,893],{"type":857,"value":892},"You can find an example of a static site generated using Nuxt JS and Cloud CMS here: ",{"type":851,"tag":894,"props":895,"children":899},"a",{"href":896,"rel":897},"https://github.com/gitana/sdk/tree/master/nuxtjs/sample",[898],"nofollow",[900],{"type":857,"value":896},{"type":851,"tag":859,"props":902,"children":903},{},[904],{"type":857,"value":905},"This site uses the Cloud CMS Nuxt Module to deliver a working example of the aforementioned capabilities.",{"type":851,"tag":907,"props":908,"children":910},"h2",{"id":909},"installation",[911],{"type":857,"value":17},{"type":851,"tag":859,"props":913,"children":914},{},[915],{"type":851,"tag":916,"props":917,"children":919},"code",{"className":918},[],[920],{"type":857,"value":921},"npm install -S cloudcms-nuxt",{"type":851,"tag":907,"props":923,"children":925},{"id":924},"nuxt",[926],{"type":857,"value":927},"Nuxt",{"type":851,"tag":859,"props":929,"children":930},{},[931,933,939],{"type":857,"value":932},"Add the module to your Nuxt configuration.  You must include the contents of your ",{"type":851,"tag":916,"props":934,"children":936},{"className":935},[],[937],{"type":857,"value":938},"gitana.json",{"type":857,"value":940}," file, like this:",{"type":851,"tag":942,"props":943,"children":945},"pre",{"code":944},"var config = require(\"./gitana.json\");\nmodule.exports = {\n    ...,\n    \"modules\": [\n        [\"cloudcms-nuxt\", config]\n    ]\n}\n",[946],{"type":851,"tag":916,"props":947,"children":948},{"__ignoreMap":809},[949],{"type":857,"value":944},{"type":851,"tag":859,"props":951,"children":952},{},[953],{"type":857,"value":954},"You can also add the following optional configuration settings:",{"type":851,"tag":942,"props":956,"children":958},{"code":957},"{\n    ...,\n    \"preview\": false,\n    \"renditions\": false,\n    \"basePageUrl\": \"http://localhost:3000\"\n}\n",[959],{"type":851,"tag":916,"props":960,"children":961},{"__ignoreMap":809},[962],{"type":857,"value":957},{"type":851,"tag":859,"props":964,"children":965},{},[966],{"type":857,"value":967},"In more detail, these are:",{"type":851,"tag":969,"props":970,"children":971},"ul",{},[972,991,1007,1017],{"type":851,"tag":873,"props":973,"children":974},{},[975,981,983,989],{"type":851,"tag":976,"props":977,"children":978},"em",{},[979],{"type":857,"value":980},"preview",{"type":857,"value":982}," - set to ",{"type":851,"tag":916,"props":984,"children":986},{"className":985},[],[987],{"type":857,"value":988},"true",{"type":857,"value":990}," to enable instant preview and branch switching capabilities",{"type":851,"tag":873,"props":992,"children":993},{},[994,999,1000,1005],{"type":851,"tag":976,"props":995,"children":996},{},[997],{"type":857,"value":998},"renditions",{"type":857,"value":982},{"type":851,"tag":916,"props":1001,"children":1003},{"className":1002},[],[1004],{"type":857,"value":988},{"type":857,"value":1006}," to track generated page renditions (per content item) and make that information available to editors for preview selection",{"type":851,"tag":873,"props":1008,"children":1009},{},[1010,1015],{"type":851,"tag":976,"props":1011,"children":1012},{},[1013],{"type":857,"value":1014},"basePageUrl",{"type":857,"value":1016}," - set to the base URL of the server for instant preview",{"type":851,"tag":873,"props":1018,"children":1019},{},[],{"type":851,"tag":907,"props":1021,"children":1023},{"id":1022},"example-usage",[1024],{"type":857,"value":1025},"Example Usage",{"type":851,"tag":859,"props":1027,"children":1028},{},[1029,1031,1037,1039,1045,1047,1054,1056,1062,1064,1069],{"type":857,"value":1030},"The ",{"type":851,"tag":916,"props":1032,"children":1034},{"className":1033},[],[1035],{"type":857,"value":1036},"$cloudcms",{"type":857,"value":1038}," context variable is available on the ",{"type":851,"tag":916,"props":1040,"children":1042},{"className":1041},[],[1043],{"type":857,"value":1044},"context",{"type":857,"value":1046},", providing access to all functions in the ",{"type":851,"tag":894,"props":1048,"children":1051},{"href":1049,"rel":1050},"https://github.com/gitana/cloudcms-javascript-driver",[898],[1052],{"type":857,"value":1053},"Cloudcms driver",{"type":857,"value":1055},". The ",{"type":851,"tag":916,"props":1057,"children":1059},{"className":1058},[],[1060],{"type":857,"value":1061},"$branch",{"type":857,"value":1063}," provides functions to your current project branch, which is by default ",{"type":851,"tag":916,"props":1065,"children":1067},{"className":1066},[],[1068],{"type":857,"value":826},{"type":857,"value":1070},".   Here is how you can read a node.",{"type":851,"tag":942,"props":1072,"children":1076},{"code":1073,"language":1074,"meta":809,"className":1075,"style":809},"async asyncData(context) {\n    let node = await context.$branch.readNode(\"myNodeId\");\n    // more awesome content functions\n}\n","javascript","language-javascript shiki shiki-themes material-theme-lighter material-theme-palenight",[1077],{"type":851,"tag":916,"props":1078,"children":1079},{"__ignoreMap":809},[1080,1108,1187,1197],{"type":851,"tag":1081,"props":1082,"children":1084},"span",{"class":1083,"line":824},"line",[1085,1091,1097,1102],{"type":851,"tag":1081,"props":1086,"children":1088},{"style":1087},"--shiki-light:#90A4AE;--shiki-dark:#BABED8",[1089],{"type":857,"value":1090},"async ",{"type":851,"tag":1081,"props":1092,"children":1094},{"style":1093},"--shiki-light:#6182B8;--shiki-dark:#82AAFF",[1095],{"type":857,"value":1096},"asyncData",{"type":851,"tag":1081,"props":1098,"children":1099},{"style":1087},[1100],{"type":857,"value":1101},"(context) ",{"type":851,"tag":1081,"props":1103,"children":1105},{"style":1104},"--shiki-light:#39ADB5;--shiki-dark:#89DDF