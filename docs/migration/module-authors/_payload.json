,"value":871},"Plugin Compatibility",{"type":813,"tag":822,"props":873,"children":874},{},[875,877,883],{"type":819,"value":876},"Nuxt 3 plugins are ",{"type":813,"tag":878,"props":879,"children":880},"strong",{},[881],{"type":819,"value":882},"not",{"type":819,"value":884}," fully backward compatible with Nuxt 2.",{"type":813,"tag":886,"props":887,"children":888},"read-more",{"to":160},[],{"type":813,"tag":866,"props":890,"children":892},{"id":891},"vue-compatibility",[893],{"type":819,"value":894},"Vue Compatibility",{"type":813,"tag":822,"props":896,"children":897},{},[898],{"type":819,"value":899},"Plugins or components using the Composition API need exclusive Vue 2 or Vue 3 support.",{"type":813,"tag":822,"props":901,"children":902},{},[903,905,913],{"type":819,"value":904},"By using ",{"type":813,"tag":842,"props":906,"children":910},{"href":907,"rel":908},"https://github.com/vueuse/vue-demi",[909],"nofollow",[911],{"type":819,"value":912},"vue-demi",{"type":819,"value":914}," they should be compatible with both Nuxt 2 and 3.",{"type":813,"tag":814,"props":916,"children":918},{"id":917},"module-migration",[919],{"type":819,"value":920},"Module Migration",{"type":813,"tag":822,"props":922,"children":923},{},[924,926,932,934,939],{"type":819,"value":925},"When Nuxt 3 users add your module, you will not have access to the module container (",{"type":813,"tag":828,"props":927,"children":929},{"className":928},[],[930],{"type":819,"value":931},"this.*",{"type":819,"value":933},") so you will need to use utilities from ",{"type":813,"tag":828,"props":935,"children":937},{"className":936},[],[938],{"type":819,"value":833},{"type":819,"value":940}," to access the container functionality.",{"type":813,"tag":866,"props":942,"children":944},{"id":943},"test-with-nuxtbridge",[945,947],{"type":819,"value":946},"Test with ",{"type":813,"tag":828,"props":948,"children":950},{"className":949},[],[951],{"type":819,"value":952},"@nuxt/bridge",{"type":813,"tag":822,"props":954,"